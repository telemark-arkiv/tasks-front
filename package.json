  "dependencies": {
    "boom": "4.3.0",
    "chairo": "3.0.0",
    "good": "7.1.0",
    "good-console": "6.2.0",
    "good-squeeze": "5.0.1",
    "hapi": "16.1.0",
    "hapi-auth-jwt2": "7.2.4",
